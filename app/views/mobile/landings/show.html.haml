- provide :titulo, ''
%section
  - if @campaing_landing.redirect_url.present?
    .display-block=link_to image_tag(@campaing_landing.image, {class: "img-responsive", style: "display: block; width: 100%;"}), @campaing_landing.redirect_url
  - else
    .display-block=image_tag(@campaing_landing.image, {class: "img-responsive", style: "display: block; width: 100%;"})