.tienda
  %ul
    - @tiendas.each do |t|
      %li
        .sucu
          .cinco
            = image_tag t.foto
          .cuatro
            %h5 
              = t.nombre
            %br
            %p 
              = t.horario
            %br
            %p 
              = t.contacto
            %br
            %p 
              = t.direccion
